<div class="content">
  <h1>List of Students</h1>
  <hr>
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Middle Name</th>
        <th>Birthday</th>
        <th>Address</th>
        <th>Student Number</th>
        <th>Contact Number</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @students.each do |ss| %>
        <tr>
          <td><%= ss.first_name %></td>
          <td><%= ss.last_name %></td>
          <td><%= ss.middle_name %></td>
          <td><%= ss.date_of_birth %></td>
          <td><%= ss.address %></td>
          <td><%= ss.student_number %></td>
          <td><%= ss.contact_number %></td>
          <td><%= ss.email %></td>
          <td>
            <%= link_to "Edit", edit_student_subject_path(student_subject.id) %>
            <%= link_to "Delete", student_subject_path(student_subject.id), method: :delete, confirm: "Are you sure?" %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
